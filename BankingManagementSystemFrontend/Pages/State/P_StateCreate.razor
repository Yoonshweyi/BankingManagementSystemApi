@page "/Setup/State/Create"

  <div class="container-xxl flex-grow-1 container-p-y shadow-sm">
        <h4 class="py-3 mb-4">
            <span class="text-muted fw-light">New State</span>
        </h4>
    
    <div class="form-group mb-3">
        <label for="exampleInputEmail1" class="mb-2">State Name</label>
        <input type="text" class="form-control" @bind-value="requestModel.StateName" placeholder="Enter State">
    </div>
    <button type="submit" class="btn btn-success mb-2" @onclick="Save">Create</button>
</div>